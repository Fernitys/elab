LAB = "turtlelab9.py"
import urllib.request
urllib.request.urlretrieve(f"http://elab.cpe.ku.ac.th/turtlelab/{LAB}",LAB)

from turtlelab9 import turtle,check

n=0
k=1
turtle.right(45)
while k != 9:
  turtle.left(45)
  k=k+1
  while n != 5:
    n = n+1
    turtle.forward(50)
  while n != 10:
    n = n+1
    turtle.backward(50)
  n=0
  
check()
